# Notion配置
# 必需：Notion集成的Token
NOTION_TOKEN=your_notion_integration_token_here

# 必需：Notion页面ID或URL
NOTION_PAGE=your_notion_page_id_or_url_here

# 微信读书配置
# 必需：微信读书Cookie（可以手动设置或通过CookieCloud获取）
WEREAD_COOKIE=your_weread_cookie_here

# CookieCloud配置（可选，用于自动获取Cookie）
# CookieCloud服务器URL
CC_URL=https://cookiecloud.malinkang.com/
# CookieCloud用户ID
CC_ID=your_cookiecloud_id
# CookieCloud密码
CC_PASSWORD=your_cookiecloud_password

# 数据库名称配置（可选，如果不设置将使用默认名称）
# 书架数据库名称
BOOK_DATABASE_NAME=书架
# 笔记数据库名称
REVIEW_DATABASE_NAME=笔记
# 划线数据库名称
BOOKMARK_DATABASE_NAME=划线
# 日数据库名称
DAY_DATABASE_NAME=日
# 周数据库名称
WEEK_DATABASE_NAME=周
# 月数据库名称
MONTH_DATABASE_NAME=月
# 年数据库名称
YEAR_DATABASE_NAME=年
# 分类数据库名称
CATEGORY_DATABASE_NAME=分类
# 作者数据库名称
AUTHOR_DATABASE_NAME=作者
# 章节数据库名称
CHAPTER_DATABASE_NAME=章节
# 阅读记录数据库名称
READ_DATABASE_NAME=阅读记录
# 设置数据库名称
SETTING_DATABASE_NAME=设置